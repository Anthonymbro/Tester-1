let person1={
    name: "Spongebob",
    age: 36,
    isemployed: true,
    sayHello: function(){
        console.log("Hello, my name is Spongebob")
    }
}

let person2={
    name: "Patrick Star",
    age: 37,
    isemployed: false,
    sayHello: function(){
        console.log("No! This is Patrick!")
    }
}

console.log(person1.age);
console.log(person2.isemployed);

person1.sayHello();
person2.sayHello();
