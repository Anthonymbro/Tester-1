

function handleClick(element){
    element.innerHTML="Clicked"
    element.style="background-color:blue;"
}




function sayHi(appearance){
    let name=prompt("What is your name?");
    document.write('<h2>Hello '+name+'! You are '+appearance+'</h2>');
}

sayHi("gorgeous!");

function avg(x,y,z){
    return
    (x+y+z)/3;
}

let res = avg(5,6,7);
console.log(res);
